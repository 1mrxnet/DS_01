#* @get /trends
# Returns hot/cold/steady players based on last 5 or 10 games
function(range = 5, status = "", limit = 10) {
range <- as.numeric(range)
data <- if (range == 10) trend10 else trend5
# Filter by status
if (status != "") {
status <- tolower(status)
if (status == "hot") data <- data[grep("On Fire", data$status, ignore.case = TRUE), ]
else if (status == "cold") data <- data[grep("Cold", data$status, ignore.case = TRUE), ]
else if (status == "steady") data <- data[grep("Steady", data$status, ignore.case = TRUE), ]
}
# Select clean fields for frontend
result <- data %>%
transmute(
player_name,
team,
position,
headshot_href,
streak_total5 = round(streak_total5, 1),
streak_total10 = round(streak_total10, 1),
status,
reason
) %>%
head(as.numeric(limit))
return(result)
}
